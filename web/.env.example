# LLM 配置（音标生成）
# 选择使用哪个模型: 'kimi' 或 'minimax'
LLM_MODEL=kimi

# Moonshot AI（Kimi）
MOONSHOT_API_KEY=your-moonshot-api-key
MOONSHOT_BASE_URL=https://api.moonshot.cn/v1

# MiniMax
MINIMAX_API_KEY=your-minimax-api-key
MINIMAX_BASE_URL=https://api.minimaxi.com/v1

# 可选：R2 公共 URL（PUBLIC_ 前缀可用于前端）
PUBLIC_R2_PUBLIC_URL=https://your-r2-public-url.com


# Supabase 项目 URL (PUBLIC_ 前缀使其可在客户端访问)
PUBLIC_SUPABASE_URL=https://your-project.supabase.co

# Supabase 匿名公钥（用于前端）
PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Supabase Service Role 密钥（仅用于后端/服务端）
# 警告：此密钥拥有完全权限，切勿暴露在前端
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Cloudflare R2 配置（音频存储）
R2_ACCOUNT_ID=your-account-id
R2_ACCESS_KEY_ID=your-access-key-id
R2_SECRET_ACCESS_KEY=your-secret-access-key
R2_BUCKET_NAME=pronunciation-audio
R2_PUBLIC_URL=https://your-r2-public-url.com
